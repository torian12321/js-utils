{
  "version": "0.0.4",
  "name": "@torian12321/js-utils",
  "description": "A collection of utility functions for JavaScript",
  "type": "module",
  "prettier": "@torian12321/prettier-config",
  "scripts": {
    "build": "vite build",
    "build:docs": "typedoc",
    "format": "eslint --fix && prettier --write .",
    "format:check": "eslint && prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "prepublishOnly": "npm run format:check && npm run test && npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torian12321/js-utils.git"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/js-utils.js",
      "require": "./dist/js-utils.cjs"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "import": "./dist/array.js",
      "require": "./dist/array.cjs"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.js",
      "require": "./dist/date.cjs"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "import": "./dist/object.js",
      "require": "./dist/object.cjs"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.js",
      "require": "./dist/string.cjs"
    },
    "./typeChecker": {
      "types": "./dist/typeChecker.d.ts",
      "import": "./dist/typeChecker.js",
      "require": "./dist/typeChecker.cjs"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "import": "./dist/url.js",
      "require": "./dist/url.cjs"
    }
  },
  "devDependencies": {
    "@torian12321/eslint-config": "0.0.11",
    "@torian12321/prettier-config": "0.0.4",
    "@torian12321/typescript-config": "0.0.5",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "typedoc": "^0.28.16",
    "typedoc-github-theme": "^0.3.1",
    "typedoc-material-theme": "^1.4.1",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  }
}
